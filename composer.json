{
    "name": "apilo/apilo-api-php",
    "description": "Apilo PHP Client",
    "version": "1.0.4",
    "homepage": "https://apilo.com/",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Apilo\\" : "src/lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Apilo\\Test\\" : "src/test/" }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "stable",
    "keywords": [
        "apilo",
        "apilo-php",
        "apilo-api",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "authors": [
        {
            "name": "Apilo",
            "email": "bok@apilo.pl"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    }
}
